    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb)attach",
            "type": "cppdbg",
            "program": "./darknet",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/install/lib",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/home/caojihua/.local/bin/gdb",
            "request": "launch",
            "miDebuggerServerAddress": "localhost:12345",

        }
    ]
}